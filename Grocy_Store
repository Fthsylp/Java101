import java.util.Scanner;

public class grocyStore {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        double pear = 2.14, apple = 3.67, tomato = 1.11, banana = 0.95, eggplant = 5.0, totalPrice;

        System.out.print("How many kilogram of pear: ");
        pear = pear*input.nextDouble();

        System.out.print("How many kilogram of apple: ");
        apple = apple*input.nextDouble();

        System.out.print("How many kilogramof tomato: ");
        tomato = tomato*input.nextDouble();

        System.out.print("How many kilogram of banana: ");
        banana = banana*input.nextDouble();

        System.out.print("How many kilogram of eggplant: ");
        eggplant = eggplant*input.nextDouble();

        totalPrice = pear+apple+tomato+banana+eggplant;
        System.out.print("Total Price: " + totalPrice + " TL");
    }
}
